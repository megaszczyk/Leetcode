    1 	public boolean twoSum(int[] nums, int k) {
    2 	    Set<Integer> set new HashSet<>();
    3 	    for(int i = 0; i < nums.length; i++) {
    4 	        int difference = k - nums[i];
    5 	        if(set.contains(difference)) {
    6 	            return true;
    7 	        }
    8 	
    9 	        set.add(nums[i]);
   10 	    }
   11 	
   12 	    return false;
   13 	}
